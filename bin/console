#!/usr/bin/env php
<?php

use App\Infrastructure\Vendor\Symfony\Kernel;
use Symfony\Bundle\FrameworkBundle\Console\Application;

require_once __DIR__.'/../vendor/autoload_runtime.php';

return fn (array $context) => (
  new Application(
    new Kernel(
      $context['APP_ENV'],
      (bool) $context['APP_DEBUG'],
    )
  )
);
